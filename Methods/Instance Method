class student:
    practical = 50
    def __init__(self):
        self.mark=""
        self.attendanceper=""
        self.sports=""
    def std1(self):
        print(self.mark)
        print(self.attendanceper)
        print(self.practical)

    def std2(self):
        print(self.mark)
        print(self.sports)
        print(self.practical)
        



rani = student()
rani.mark=90
rani.attendanceper=60
rani.std1()

vikram =student()
vikram.mark=75
vikram.sports=55
vikram.std2()


        
